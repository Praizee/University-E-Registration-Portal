myApp.controller("cutoffCtrl", function($scope, $http, $localStorage){
	
	$scope.bio_data = $localStorage.bio_data;
	
	$scope.jambRegNo = $scope.bio_data.jambRegNo;
// get cut off marks from back end
	$http.post("backend/sqlprocesses.php?getCutOffMarks=yes")
		.success(function(response){
			
			if(response.status == "ok")
			{
				
				$scope.data = response.data;
				$scope.list = response.data;
				$scope.lengthOfdata = $scope.data.length;
			}
			else if(response.status == "not ok")	
			{
				items = "Unable to retrieve department";
				modalMessage(items);
			}
			else
			{
				
			
			}
		})
		.error(function(errorMessage){
		});	
		

	});