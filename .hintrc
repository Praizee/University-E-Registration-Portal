{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off",
        "duplicate-id-aria": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "img[loading]"
        ]
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-input-field-name": "off",
        "aria-required-children": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior",
          "backdrop-filter"
        ]
      }
    ],
    "no-protocol-relative-urls": "off"
  }
}